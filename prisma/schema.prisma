generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  code        String   @id @unique @db.VarChar(8)
  url         String
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
  lastClicked DateTime?
  expiresAt   DateTime?  
}
